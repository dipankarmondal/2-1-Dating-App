{
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules"
  ]
}